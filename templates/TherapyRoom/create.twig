{% extends "base.html.twig" %}
{% block body %}
    {% embed 'Structure/Form/create_structure.html.twig' with {'title': data} %}
        {% block form_content %}
            {{ form_start(form) }}
            <div class="form-group">
                {{ form_label(form.number) }}
                {{ form_widget(form.number, {'attr': {'class': 'form-control'}}) }}
            </div>
            <div class="form-group">
                {{ form_label(form.name) }}
                {{ form_widget(form.name, {'attr': {'class': 'form-control'}}) }}
            </div>
            <div class="form-group">
                {{ form_label(form.session) }}
                {{ form_widget(form.session, {'attr': {'class': 'form-control'}}) }}
            </div>
            <div class="form-group">
                {{ form_label(form.attendance_limit) }}
                {{ form_widget(form.attendance_limit, {'attr': {'class': 'form-control'}}) }}
            </div>
            <div class="text-center mt-4">
                {{ form_widget(form.save) }}
            </div>
            {{ form_end(form) }}
        {% endblock %}
    {% endembed %}
{% endblock %}